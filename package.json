{
    "name": "vuepress-blog-template",
    "version": "2021.0.0",
    "description": "Blog Title",
    "scripts": {
        "postinstall": "scripts/postinstall.sh",
        "dev": "vuepress dev src",
        "build": "vuepress build src",
        "start": "npm run build && npm run serve",
        "serve": "serve --listen 8080 --no-clipboard dist",
        "lint": "eslint --fix --ignore-path .gitignore \"src/.vuepress/**/*.js\" \"src/.vuepress/**/*.vue\" \"cypress/**/*.js\"",
        "test:all": "npm run test:unit && npm run test:e2e",
        "test:e2e": "cypress run --quiet",
        "test:e2e:debug": "cypress open",
        "test:unit": "jest",
        "test:unit:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --no-cache --runInBand",
        "pretest": "npm run lint",
        "test": "start-server-and-test start 8080 test:all"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dvuckovic/vuepress-blog-template.git"
    },
    "keywords": [
        "vuepress",
        "blog",
        "template"
    ],
    "author": "Dusan Vuckovic <dusan@dvuckovic.com>",
    "license": "WTFPL",
    "homepage": "https://dvuckovic.com",
    "devDependencies": {
        "@babel/plugin-transform-runtime": "^7.12.10",
        "@popperjs/core": "^2.5.4",
        "@vue/test-utils": "^1.1.2",
        "@vuepress/plugin-google-analytics": "^1.7.1",
        "@vuepress/theme-blog": "^2.3.3",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^26.6.3",
        "bootstrap": "^5.0.0-beta2",
        "chart.js": "^2.9.4",
        "chartjs-plugin-deferred": "^1.0.1",
        "cypress": "^6.2.0",
        "cypress-pipe": "^2.0.0",
        "dotenv": "^8.2.0",
        "eslint": "^7.15.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-chai-friendly": "^0.6.0",
        "eslint-plugin-cypress": "^2.11.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-vue": "^7.2.0",
        "jest": "^26.6.3",
        "jest-canvas-mock": "^2.3.0",
        "lodash": "^4.17.20",
        "masonry-layout": "^4.2.2",
        "medium-zoom": "^1.0.6",
        "moment": "^2.29.1",
        "node-sass": "^5.0.0",
        "page-flip": "^2.0.0",
        "papaparse": "^5.3.0",
        "photo-sphere-viewer": "^4.0.7",
        "photoswipe": "^4.1.3",
        "sass-loader": "^10.1.0",
        "serve": "^11.3.2",
        "sprintf-js": "^1.1.2",
        "start-server-and-test": "^1.11.7",
        "striptags": "^3.1.1",
        "vue-in-viewport-mixin": "^2.0.1",
        "vue-jest": "^3.0.7",
        "vue-plugin-load-script": "^1.3.2",
        "vue2-google-maps": "^0.10.7",
        "vuepress": "^1.7.1",
        "vuepress-plugin-dehydrate": "^1.1.5",
        "vuex": "^3.6.0",
        "vuex-router-sync": "^5.0.0"
    }
}
